{
  "compilerOptions": {
    "noEmit": true,
    "module": "preserve",
    "target": "es2021",
    "lib": [
      "es2021",
      "DOM"
    ],
    "allowJs": true,
    "checkJs": true,
    "strict": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
  },
  "exclude": [
    "_site/",
    ".yarn/",
    "node_modules/",
    "src/lib/"
  ]
}
